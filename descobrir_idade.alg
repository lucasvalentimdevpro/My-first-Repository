algoritmo "Creuza01"
var
   ano_atual, ano_nasc, idade : Inteiro
inicio
      Escreva ("Qual é o seu ano atual? ")
      Leia (ano_atual)
      Escreva ("Qual o ano em que nasceu? ")
      Leia (ano_nasc)
      idade <- (ano_atual - ano_nasc)
      Escreva ("Em 2025 você terá", idade," anos")
fimalgoritmo
